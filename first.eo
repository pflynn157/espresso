routine main(args : str[]) is
    var x  : int := 4;
    var a1 : int := x & 5;
    var a2 : int := x | 5;
    var a3 : int := x ^ 5;
    var a4 : int := x << 2;
    var a5 : int := x >> 2;
    
    println(x);
    println(a1);
    println(a2);
    println(a3);
    println(a4);
    println(a5);
end
